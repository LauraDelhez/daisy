f (DM0, DM1, M0, t0, t1) -> S

M_max_0 = M_max (DM0)

M_max_1 = M_max (DM1)

growth = new_M_max - old_M_max

delta_t = t1 - t0

growth_rate = growth / delta_t

if (M0 + growth >= M_max_1 || M0 >= M_max_1)

  S = (M_max_1 - M0) / delta_t

else

  dM/dt = growth_rate + fill_rate (M_max_1 - M)

  M (t) = C1 exp (- fill_rate * t) + growth_rate / fill_rate + M_max_1

  M0 = C1 + growth_rate / fill_rate + M_max_1

  C1 = M0 - growth_rate / fill_rate - M_max_1


